PORT=10000
NODE_ENV=production
N8N_HOST=0.0.0.0
N8N_PORT=10000
N8N_PROTOCOL=https
N8N_LOG_LEVEL=info

# Authentication (SENSITIVE)
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=quincycampbell9@gmail.com
N8N_BASIC_AUTH_PASSWORD=Green126!

# Security Keys (SENSITIVE)
N8N_ENCRYPTION_KEY=1c4e4906-16c4-4cf4-9792-ec54d2129214
N8N_SESSION_SECRET=render-n8n-session-2025-secure-key-xyz789

# URLs and Webhooks (Update with your actual Render URL)
N8N_EDITOR_BASE_URL=https://n8n-deploy-xv1b.onrender.com
WEBHOOK_URL=https://n8n-deploy-xv1b.onrender.com
N8N_WEBHOOK_URL=https://n8n-deploy-xv1b.onrender.com

# Proxy Configuration
N8N_TRUST_PROXY=true
EXPRESS_TRUST_PROXY=true
TRUST_PROXY=1

# Database Configuration
DB_TYPE=sqlite
DB_SQLITE_DATABASE=/home/node/.n8n/database.sqlite
DB_SQLITE_POOL_SIZE=1

# Runtime Configuration
N8N_RUNNERS_ENABLED=true
N8N_BLOCK_ENV_ACCESS_IN_NODE=false
N8N_USER_FOLDER=/home/node/.n8n
N8N_SECURE_COOKIE=false
N8N_JWT_AUTH_HEADER=authorization

# Timezone Configuration
GENERIC_TIMEZONE=America/New_York
TZ=America/New_York

# Execution Configuration
EXECUTIONS_DATA_PRUNE=true
EXECUTIONS_DATA_MAX_AGE=168
EXECUTIONS_DATA_SAVE_ON_ERROR=all
EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
EXECUTIONS_DATA_SAVE_MANUAL_EXECUTIONS=true

# Features Configuration
N8N_METRICS=true
N8N_DIAGNOSTICS_ENABLED=false
N8N_VERSION_NOTIFICATIONS_ENABLED=false
N8N_TEMPLATES_ENABLED=true
N8N_ONBOARDING_FLOW_DISABLED=true
N8N_PERSONALIZATION_ENABLED=false
